{
    "main": "index.js",
    "name": "scipm.hex_edit",
    "version": "0.1.0",
    "description": "basic hex viewer and editor",
    "license": "GPL-3.0",
    "repository": {
        "type": "git",
        "url": "git://github.com/aminassian/scipm.hex_edit.git"
    },
    "bugs": {
        "type": "git",
        "url": "git://github.com/aminassian/scipm.hex_edit/issues"
    },
    "keywords": [
        "scipm-package"
    ],
    "dependencies": {
        "scipm": "^1.0.0"
    },
    "scipmchild": {
        "version": "1",
        "scipmDependencies": {
            "scipm.extman": null
        },
        "luaRequire": [],
        "SciTEStartup": {
            "_use": "dofile",
            "raw": null,
            "filePath": null,
            "dofile": [
                "scite",
                "main.lua"
            ],
            "luaReportExtend": "scipm.core.printInfoPackage(\"scipm.hex_edit\");",
            "infoLuaFunctions": [
                {
                    "name": "scipm.hex_edit.HexEditor()"
                }
            ],
            "infoFiles": [
                {
                    "path": [
                        "README.md"
                    ]
                },
                {
                    "path": [
                        "package.json"
                    ]
                },
                {
                    "path": [
                        "scite",
                        "main.lua"
                    ]
                }
            ],
            "infoProps": [
            ]
        },
        "actions": {
            "hexEdit": {
                "title": "HexEditor",
                "description": "the hex editor to open the current file",
                "pattern": [
                    "*"
                ],
                "addToolsMenu": {
                    "allow": true,
                    "addContextMenu": false,
                    "command": {
                        "shortcuts": null,
                        "subsystem": "luadirector",
                        "cmd": "dostring scipm.hex_edit.HexEditor()",
                        "isFilter": null,
                        "saveBefore": "2",
                        "input": null,
                        "replaceSelection": null,
                        "quiet": null,
                        "mode": null
                    }
                }
            }
        }
    }
}
